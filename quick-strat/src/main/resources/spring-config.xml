<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<context:component-scan	base-package="com.quick.start" />
	<import resource="classpath:/helloworld-config.xml"/>
	
	
	<bean id="helloWorldController"	class="com.quick.start.controllers.HelloWorldController">
			<property name="helloService" ref="helloWorldServiceDefault"></property>
			<property name="helloWorldSpanichService" ref="helloWorldSpanichService"></property>
			<property name="helloWorldEnglish" ref="english"></property>
	</bean>
		

</beans>
